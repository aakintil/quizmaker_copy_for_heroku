<%= form_for @user_role do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :user_id %><br />
    <%= f.collection_select :user_id, User.all, :id, :name, :prompt  => 'Select...' %>
  </p>
  <p>
    <%= f.label :role_id %><br />
    <%= f.collection_select :role_id, Role.all, :id, :name, :prompt => 'Select...' %>
  </p>
  <p><%= f.submit %></p>
<% end %>
